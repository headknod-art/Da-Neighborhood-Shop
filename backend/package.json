{
  "name": "da-neighborhood-backend",
  "version": "1.0.0",
  "description": "Backend API for Da Neighborhood Shop",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "npm run health-check && npm run init-db && tsx watch src/index.ts",
    "build": "tsc",
    "start": "npm run health-check && npm run init-db && node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "health-check": "tsx scripts/healthCheck.ts",
    "init-db": "tsx scripts/initDb.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@prisma/client": "^6.19.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "prisma": "^6.19.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.1"
  }
}
